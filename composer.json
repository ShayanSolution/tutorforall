{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.0|^8.0",
        "laravel/lumen-framework": "^8.0",
        "vlucas/phpdotenv": "^5.2",
        "ramsey/uuid": "^4.0",
        "league/fractal": "^0.17.0",
        "dusterio/lumen-passport": "^0.3.3",
        "flipbox/lumen-generator": "^6.0",
        "twilio/sdk": "^5.19",
        "brozot/laravel-fcm": "1.3.1",
        "apility/laravel-fcm": "dev-master",
        "doctrine/dbal": "^2.10",
        "jcf/geocode": "~1.3",
        "sentry/sentry-laravel": "^2.2",
        "lcobucci/jwt": "3.3.*",
        "monolog/monolog": "^2.0",
        "illuminate/support": "8.27.0 as 6.0.0",
        "beyondcode/laravel-websockets": "^1.9",
        "fruitcake/laravel-cors": "^2.0",
        "illuminate/mail": "^8.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.7",
        "phpunit/phpunit": "~6.5",
        "mockery/mockery": "~1.0"
    },
    "autoload": {
        "classmap": [
            "app/Library/"
        ],
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:zach-lum-fm/laravel-fcm.git"
        }
    ]
}
